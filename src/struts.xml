		<action name="loginq" class="lr.LoRe" method = "Log">
			<result name="boss">/Boss.jsp</result>		
			<result name="employee">/Employee.jsp</result>
			<result name="fail">/Login.jsp</result>	
		</action>
				
	</package>
	<package name="lrqq" namespace="/" extends="struts-default">
